<form action="/landmarks/<%= @landmark.id %>" method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <label>Name:</label>
  <input type="text" name="landmark[name]" placeholder="name" value="<%= @landmark.name%>">
  <label>Year:</label>
  <input type="text" name="landmark[year_completed]" placeholder="year_completed" value="<%= @landmark.year_completed%>">
  <input type="submit" value="Edit">
</form>
